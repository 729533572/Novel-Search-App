        android:id="@+id/btn_confirm8"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="固定价格称重" />
    <Button
        android:id="@+id/btn_confirm9"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/confirm" />


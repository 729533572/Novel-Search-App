<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="PagerSlidingTabStrip">
        <attr name="pstsIndicatorColor" format="color" />
        <attr name="pstsUnderlineColor" format="color" />
        <attr name="pstsDividerColor" format="color" />
        <attr name="pstsDividerWidth" format="dimension" />
        <attr name="pstsTabTextSize" format="dimension" />
        <attr name="pstsIndicatorPadding" format="dimension" />
        <attr name="pstsIndicatorHeight" format="dimension" />
        <attr name="pstsUnderlineHeight" format="dimension" />
        <attr name="pstsDividerPadding" format="dimension" />
        <attr name="pstsTabPaddingLeftRight" format="dimension" />
        <attr name="pstsScrollOffset" format="dimension" />
        <attr name="pstsTabBackground" format="reference" />
        <attr name="pstsShouldExpand" format="boolean" />
        <attr name="pstsTextAllCaps" format="boolean" />
    </declare-styleable>

    <declare-styleable name="CalendarView">
        <attr name="EnableDateColor" format="reference|color"/>
        <attr name="UnableDateColor" format="reference|color"/>
        <attr name="CurrentColor" format="reference|color"/>
        <attr name="CurrentSize" format="dimension"/>
        <attr name="WeekDayColor" format="reference|color"/>
        <attr name="SelectBGColor" format="reference|color"/>
        <attr name="CircleColor" format="reference|color"/>
        <attr name="RelaxColor" format="reference|color"/>
        <attr name="WorkColor" format="reference|color"/>
        <attr name="PriceColor" format="reference|color"/>
        <attr name="DateSize" format="dimension"/>
        <attr name="PriceSize" format="dimension"/>
        <attr name="DateHeight" format="dimension"/>
    </declare-styleable>

    <declare-styleable name="InputView">
        <attr name="titleText" format="string" />
        <attr name="secondTitleText" format="string" />
        <attr name="inputHint" format="string" />
        <attr name="inputText" format="string" />
        <attr name="inputMode" format="integer" />
    </declare-styleable>

    <declare-styleable name="dashgridview">
        <attr name="is_dash" format="boolean"/>
        <attr name="solid_line_color" format="color"/>
    </declare-styleable>
</resources>
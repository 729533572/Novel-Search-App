<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.yonyou.hhtpos">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".application.MyApplication"
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity
            android:name=".ui.dinner.wm.ACT_TakeOut"
            android:screenOrientation="landscape"
            android:theme="@style/style_translucent_activity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <!--绑定门店 -->
        <activity
            android:name=".ui.store.ACT_BindStore"
            android:screenOrientation="landscape"
            android:theme="@style/style_translucent_activity"/>

        <!--登录页面 -->
        <activity
            android:name=".ui.login.ACT_Login"
            android:screenOrientation="landscape"
            android:theme="@style/style_translucent_activity"/>

        <!--验证手机号 -->
        <activity
            android:name=".ui.activation.ACT_VerifyPhone"
            android:screenOrientation="landscape"
            android:theme="@style/style_translucent_activity"/>

        <!--设置新密码 -->
        <activity
            android:name=".ui.login.ACT_ResetNewPwd"
            android:screenOrientation="landscape"
            android:theme="@style/style_translucent_activity"/>

        <!--首页 -->
        <activity
            android:name=".ui.home.ACT_Home"
            android:screenOrientation="landscape"
            android:theme="@style/style_translucent_activity"/>

        <!--预定总览 -->
        <activity
            android:name=".ui.book.ACT_BookPreview"
            android:screenOrientation="landscape"
            android:theme="@style/style_translucent_activity"/>

        <!--待确认订单 -->
        <activity
            android:name=".ui.reserve.ACT_TobeConfirmOrder"
            android:screenOrientation="landscape"
            android:theme="@style/style_translucent_activity"/>

        <!--外带 -->
        <activity
            android:name=".ui.dinner.wd.ACT_Packing"
            android:screenOrientation="landscape"
            android:theme="@style/style_translucent_activity"/>

        <!--&lt;!&ndash;外带 &ndash;&gt;-->
        <!--<activity-->
            <!--android:name=".ui.dinner.wm.ACT_TakeOut"-->
            <!--android:screenOrientation="landscape"-->
            <!--android:theme="@style/style_translucent_activity"/>-->

    </application>

</manifest>